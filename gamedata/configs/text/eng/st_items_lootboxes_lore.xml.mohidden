	<string id="st_lootbox_docs_sheet">
		<text>Loose Document</text>
	</string>
	
	<string id="st_lootbox_docs_sheets">
		<text>Old Documents</text>
	</string>

	<string id="st_lootbox_docs_notebook">
		<text>Notebook</text>
	</string>
	
	<string id="st_lootbox_docs_folder">
		<text>Folder with Documents</text>
