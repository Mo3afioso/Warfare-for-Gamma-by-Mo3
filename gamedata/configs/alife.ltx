[alife]
    schedule_min				= 1							; milliseconds
    schedule_max				= 1							; milliseconds
    process_time				= 1800						; microseconds
	objects_per_update			= 35
    update_monster_factor		= 0.1
    time_factor					= 6							; how many in-game seconds will pass per 1 real-life second
    normal_time_factor			= 10

;-- Online Switching Radius: objects inside this radius will switch online (game objects)
    switch_distance				= 350						; [meters] default distance (engine)
    switch_factor				= 0.1						; no idea what this does					
    auto_switch					= false                    ; Enable automatic distance switching (xr_patch.script) for optimizing performance
	auto_switch_timer			= 5000                      ; [milliseconds] (used if auto_switch is on) time required to switch from start to normal distance after loading
	auto_switch_distance_start	= 2000                      ; [meters] (used if auto_switch is on) switch_distance will set to this value upon loading for few seconds
	auto_switch_distance_normal	= 350                       ; [meters] (used if auto_switch is on) switch_distance will set to this value in normal case

	start_time					= 08:00:00
    start_date					= 26.10.2018
    autosave_interval			= 00:15:00
    delay_autosave_interval		= 00:00:30
	
	start_game_callback			= _G.start_game_callback	; on starting new game or loading saved one
